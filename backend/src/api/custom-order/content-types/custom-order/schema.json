{
  "kind": "collectionType",
  "collectionName": "custom_orders",
  "info": {
    "singularName": "custom-order",
    "pluralName": "custom-orders",
    "displayName": "Custom Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "telefone": {
      "type": "string"
    },
    "cardNome": {
      "type": "string",
      "required": true
    },
    "cardDescricao": {
      "type": "text"
    },
    "categoria": {
      "type": "string"
    },
    "tipo": {
      "type": "string"
    },
    "raridade": {
      "type": "string"
    },
    "condicao": {
      "type": "string"
    },
    "nacionalidade": {
      "type": "string"
    },
    "lacrado": {
      "type": "boolean",
      "default": false
    },
    "setNome": {
      "type": "string"
    },
    "numeroCarta": {
      "type": "string"
    },
    "quantidade": {
      "type": "integer",
      "default": 1
    },
    "observacoes": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pendente", "analisando", "orcamento_enviado", "aprovado", "concluido", "cancelado"],
      "default": "pendente"
    }
  }
}